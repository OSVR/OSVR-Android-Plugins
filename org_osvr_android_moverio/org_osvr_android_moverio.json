{
  "deviceVendor": "Epson",
  "deviceName": "Epson Moverio BT-300 Device Plugin",
  "author": "Jeremy Bell <jeremy@sensics.com>",
  "version": 1,
  "lastModified": "2015-01-30T21:13:07.585Z",
  "interfaces": {
    "tracker": {
      "count": 2,
      "bounded": true,
      "position": false,
      "orientation": true
    },
    "button": {
      "count": 9
    },
    "analog": {
      "count": 2
    }
  },
  "semantic": {
    "hmd": {
      "$target": "tracker/0",
      "tap": "button/8"
    },
    "controller": {
      "0": "button/0",
      "1": "button/1",
      "2": "button/2",
      "3": "button/3",
      "4": "button/4",
      "volumeUp": "button/5",
      "volumeDown": "button/6",
      "back": "button/7"
    },
    "mouse": {
      "x": "analog/0",
      "y": "analog/1"
    },
    "base": "tracker/1"
  },
  "automaticAliases": {
    "/me/head": "semantic/hmd/*",
    "/me/hand": "semantic/base",
    "/controller/left": "semantic/controller/*",
    "/mouse": "semantic/mouse/*"
  }
}
